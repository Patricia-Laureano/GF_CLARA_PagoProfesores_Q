@{
    // ________________________________ IMPORTAR DATOS PBI ________________________________

    ViewBag.Title = "Start";
}

<div class="row">
    <ol class="breadcrumb pull-right">
        <li><a href="#">Proceso para Docentes Multicampus</a></li>

    </ol>
</div>

<!-- begin row -->
<div class="row">
    <!-- begin col-12 -->
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a data-click="panel-collapse" class="btn btn-xs btn-icon btn-circle btn-warning" href="javascript:;"><i class="fa fa-minus"></i></a>
                </div>
                <h4 class="panel-title">Proceso para Docentes Multicampus</h4>
            </div>
            <div class="panel-body">
                <ul class="nav nav-tabs">
                    <li class="active"><a href="/ImportarDatosPBI" aria-expanded="true">Importación de PA multicampus</a></li>
                    <li class=""><a href="/CargarDatosComplementarios" aria-expanded="false">Subir archivo Excel DI</a></li>
                    <li class=""><a href="/VerDatosComplementarios" aria-expanded="false">Sabana de datos</a></li>
                    <li class=""><a href="/ImportarDatosExcel" aria-expanded="false">Importación de datos desde Excel</a></li>

                </ul>
                <div class="panel-body" id="seccion1">
                    <form action="/" method="POST" onload="formPage.clean();" name="f1" id="formElement">
                        <fieldset>
                            <legend class="pull-left width-full small">Utilice este procedimiento para importar y actualizar información de Profesores de Banner en el sistema. <a href="javascript:void(0)" onclick="ayuda('importprofesores.html')">Más ayuda.</a></legend>

                            <div class="row">
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label for="Ciclo">A&ntilde;o escolar</label>
                                        <select id="Ciclo" class="form-control"></select>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label for="Periodo">Periodo</label>
                                        <select id="Periodo" class="form-control"></select>
                                    </div>
                                </div>


                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label for="fechai">Fecha Inicio </label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" id="fechai">
                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label for="fechaf">Fecha Fin</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" id="fechaf">
                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                        </div>
                                    </div>
                                </div>



                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="sedesCheck">Sedes*</label>
                                        <div class="input-group">
                                            <div id="sedesCheck"></div>
                                        </div>
                                    </div>
                                </div>
                                <input type="hidden" id="chesSeleccionados" value="0" />
                            </div>

                            <div class="row right">

                                <button type="button" id="formbtnConsultar" class="btn btn-sm btn-primary" onclick="formPage.consultar();">Consultar</button>
                               
                            </div>

                            @Html.Raw(ViewBag.BlockingPanel_1)

                        </fieldset>
                    </form>
                </div>
            </div>
            </div>
            <!-- end panel -->
        </div>
    <!-- end col-12 -->
</div>
<!-- end row -->
<!-- begin row -->
<div class="row">
    <!-- begin col-12 -->
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a data-click="panel-reload" class="btn btn-xs btn-icon btn-circle btn-success" href="javascript:;"><i class="fa fa-repeat"></i></a>
                    <a data-click="panel-collapse" class="btn btn-xs btn-icon btn-circle btn-warning" href="javascript:;"><i class="fa fa-minus"></i></a>
                </div>
                <h4 class="panel-title">Listado de Personas <span class="loader-min-bar"></span></h4>
            </div>
            <div class="panel-body">
                <div id="datatable">
                    @Html.Raw(ViewBag.DataTable)
                </div>
                @Html.Raw(ViewBag.BlockingPanel_2)
            </div>
        </div>
        <!-- end panel -->
    </div>
    <!-- end col-12 -->
</div>
<!-- end row -->
<!-- #modal-dialog CONFIRM DELETE -->
<div class="modal fade" id="modal-delete">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Borrar datos</h4>
            </div>
            <div class="modal-body">
                Se eliminar&aacute; el dato seleccionado. ¿Esta seguro?
            </div>
            <div class="modal-footer">
                <a href="javascript:;" class="btn btn-sm btn-white" data-dismiss="modal">Cancelar</a>
                <a href="javascript:formPage.delete(true);" class="btn btn-sm btn-danger">Eliminar</a>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript"></script>

<!-- #modal DETALLES-CONFLICTOS -->
<div class="modal fade" id="modal-detalles-conflictos" style="display: none;" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Conflictos al cargar la información desde BANNER.</h4>
            </div>
            <div class="modal-body">
                <div id="divDetallesConflictos"></div>
            </div>
            <div class="modal-footer">
                <a href="javascript:;" class="btn btn-sm btn-white" data-dismiss="modal">Cerrar</a>
                <!--<a href="javascript:formPage.delete(true);" class="btn btn-sm btn-danger">Eliminar</a>-->
            </div>
        </div>
    </div>
</div>